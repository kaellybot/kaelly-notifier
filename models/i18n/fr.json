{
    "almanax": {
        "title": "Almanax du {{ .date }}",
        "bonus": {
            "title": ":cyclone: Bonus"
        },
        "url": "https://www.krosmoz.com/fr/almanax/{{ .date }}",
        "tribute": {
            "title": ":bouquet: Offrande",
            "description": "{{ .emoji }} **{{ .quantity }}**x {{ .item }}"
        },
        "reward": {
            "title": ":moneybag: Gains",
            "description": "**{{ .reward }}** {{ .kamaIcon }}"
        }
    },
    "game": {
        "message": "Je suis à jour avec la version **{{ .version }}** de {{ .game }} !"
    },
    "guild": {
        "create": "[NEW] **{{ .name }}**, +{{ .memberCount }} utilisateurs",
        "delete": "[LOSE] **{{ .name }}**, -{{ .memberCount }} utilisateurs"
    },
    "set": {
        "message": "{{ if .sets }}Les vignettes de panoplies {{ .game }} sont manquantes pour les IDs suivants :\n{{ range .sets}} * {{ . }}\n{{ end }}{{ else }}Aucune vignette de panoplie n'est manquante pour {{ .game }} !{{ end }}"
    },
    "default": {
        "footer": "{{ .name }} • Message automatique{{ if .date }} • {{ .date }}{{ end }}"
    }
}
