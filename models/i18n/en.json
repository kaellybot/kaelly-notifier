{
    "almanax": {
        "title": "Almanax of {{ .date }}",
        "bonus": {
            "title": ":cyclone: Bonus"
        },
        "url": "https://www.krosmoz.com/en/almanax/{{ .date }}",
        "tribute": {
            "title": ":bouquet: Tribute",
            "description": "{{ .emoji }} **{{ .quantity }}**x {{ .item }}"
        },
        "reward": {
            "title": ":moneybag: Rewards",
            "description": "**{{ .reward }}** {{ .kamaIcon }}"
        }
    },
    "game": {
        "message": "I am up-to-date with {{ .game }} **{{ .version }}**!"
    },
    "guild": {
        "create": "[NEW] **{{ .name }}**, +{{ .memberCount }} users",
        "delete": "[LOSE] **{{ .name }}**, -{{ .memberCount }} users"
    },
    "set": {
        "message": "{{ if .sets }}{{ .game }} thumbnails sets are missing for following IDs:\n{{ range .sets}} * {{ . }}\n{{ end }}{{ else }}{{ .game }} thumbnails sets are all up-to-date!{{ end }}"
    },
    "default": {
        "footer": "{{ .name }} • Automatic message{{ if .date }} • {{ .date }}{{ end }}"
    }   
}
